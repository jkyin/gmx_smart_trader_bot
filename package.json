{
  "name": "gmx_smart_trader_bot",
  "version": "0.0.1",
  "description": "",
  "author": "jkyin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm build:graphql && pnpm typechain:generate && nest build",
    "build:graphql": "graphclient build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "graphiql": "graphclient serve-dev",
    "typechain:generate": "typechain --target=ethers-v6 --out-dir src/gmx.house/contracts/types 'src/gmx.house/contracts/abis/*.json'",
    "typechain:typecheck": "pnpm typechain:generate && tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/terminus": "^9.2.2",
    "@types/lodash": "^4.14.194",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "binance": "^2.8.0",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.7",
    "ethers": "^6.3.0",
    "fast-array-diff": "^1.1.0",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.2",
    "nestjs-telegraf": "^2.6.3",
    "reflect-metadata": "^0.1.13",
    "telegraf": "^4.12.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^2.2.22",
    "@graphprotocol/client-polling-live": "^1.1.1",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@graphql-mesh/cache-localforage": "^0.7.20",
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/graphql": "^0.34.17",
    "@graphql-mesh/http": "^0.3.29",
    "@graphql-mesh/merger-bare": "^0.16.25",
    "@graphql-mesh/runtime": "^0.46.24",
    "@graphql-mesh/store": "^0.9.20",
    "@graphql-mesh/utils": "^0.43.23",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@typechain/ethers-v6": "^0.3.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@whatwg-node/fetch": "^0.8.8",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "^16.6.0",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
