{
  "name": "gmx_smart_trader_bot",
  "version": "0.0.2",
  "description": "",
  "author": "jkyin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && npm run typechain:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "graphiql": "graphclient serve-dev",
    "typechain:generate": "typechain --target=ethers-v6 --out-dir src/gmx.house/contracts/types 'src/gmx.house/contracts/abis/*.json'",
    "typechain:typecheck": "npm run typechain:generate && tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^1.4.2",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/terminus": "^9.2.2",
    "@types/lodash": "^4.14.195",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "binance": "^2.8.2",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.8",
    "ethers": "^6.6.0",
    "fast-array-diff": "^1.1.0",
    "lodash": "^4.17.21",
    "logform": "^2.5.1",
    "nestjs-telegraf": "^2.6.3",
    "reflect-metadata": "^0.1.13",
    "telegraf": "^4.12.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@typechain/ethers-v6": "^0.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "^2.5.3",
    "typechain": "^8.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "~18.16.0",
    "npm": "~9.5.1"
  }
}
